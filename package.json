    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "publishConfig": {
    "access": "public"
  },
  "name": "@xdoer/chokidar",
  "author": "xdoer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xdoer/chokidar.git"
  },
  "bugs": {
    "url": "https://github.com/xdoer/chokidar/issues"
  },
  "browserslist": [
    "> 5%"
  ],
  "module": "dist/chokidar.esm.js",
  "size-limit": [
    {
      "path": "dist/chokidar.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/chokidar.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "chokidar": "^3.5.2"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "size-limit": "^4.10.2",
    "ts-lib": "^0.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "gitHead": "cc76247b6910e3059e449f5927a43a3897396d7b"
